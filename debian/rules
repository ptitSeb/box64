#!/usr/bin/make -f

%:
	dh $@  

override_dh_auto_configure:
	dh_auto_configure -- \
	      -DCMAKE_BUILD_TYPE=RelWithDebInfo \
	      -DARM_DYNAREC=1 \
		  -DNOGIT=1

override_dh_shlibdeps:
	dh_shlibdeps --exclude=libgcc_s.so.1 --exclude=libpng12.so.0 --exclude=libstdc++.so.5 --exclude=libstdc++.so.6
